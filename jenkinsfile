node('jdk11-mvn3.8.4') {
    stage('git') {
        git 'https://github.com/GitPracticeRepo/java11-examples.git'
pipeline {
    agent { label 'jdk11-mvn3.8.4' }
    triggers { upstream(upstreamProjects: 'starterproject', threshold: hudson.model.Result.SUCCESS) }
    stages {
        stage('scm') {
            steps {
                git 'https://github.com/reddynalla/gittest.git'
            }
        }
        stage('build') {
            steps {
                sh '/usr/local/apache-maven-3.8.5/bin/mvn clean package'
            }
        }
	}
	}
	}
	}

